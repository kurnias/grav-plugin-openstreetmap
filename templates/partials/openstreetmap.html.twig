<div id="map" class="openstreetmap" 
     data-lat="{{ lat }}" 
     data-lng="{{ lng }}" 
     data-zoom="{{ zoom|default(13) }}"
     data-marker-lat="{{ marker_lat|default('') }}"
     data-marker-lng="{{ marker_lng|default('') }}"
     data-marker-popup="{{ marker_popup|default('') }}"
     style="height: 400px;"></div>
